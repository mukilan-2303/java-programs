import java.util.*;
import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        String name= sc.nextLine();
        int age= sc.nextInt();
        System.out.print("My name is "+name+"\n");
        System.out.println("My age is "+age);
        sc.close();
    }
}
