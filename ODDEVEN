import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
      Scanner sc= new Scanner(System.in);
        int a=sc.nextInt();
        if(a%2==0)
            System.out.print("EVEN");
        else
        {
            System.out.println("ODD");
        }
        sc.close();
        
    }
}
