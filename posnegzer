import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
       Scanner sc =new Scanner(System.in);
        int in = sc.nextInt();
        if(in>0){
            System.out.println("Positive");        
        }
    else if(in==0){
                    System.out.println("Zero");
        }
        else{
                        System.out.println("Negative");        

        }
}
}
