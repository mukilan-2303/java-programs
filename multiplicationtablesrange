import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int multiplicant = sc.nextInt();
        for(int i=1;i<=n;i++){
          System.out.println(i +" * "+ multiplicant+" = " + i*multiplicant);
        }
        sc.close();
    }
}
